function getIEVersion(){var e=99;if("Microsoft Internet Explorer"==navigator.appName){var r=navigator.userAgent;null!=new RegExp("MSIE ([0-9]{1,}[.0-9]{0,})").exec(r)&&(e=parseFloat(RegExp.$1))}return e}getIEVersion()<=9&&$(".z-top-container").after("<div class='ie9-tips'>为了保障你的账号安全和最佳浏览体验，该页面即将不再支持IE9及以下IE版本浏览器访问，建议你升级到IE浏览器最新版本，或使用Chrome等浏览器最新版本。</div>")&&$(".ie9-tips").css({"font-size":"18px","background-color":"#00a1d6","text-align":"center",padding:"10px 0",color:"#fff"});